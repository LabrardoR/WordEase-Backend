<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.head.wordeasebackend.mapper.WordMapper">

    <resultMap id="BaseResultMap" type="com.head.wordeasebackend.model.entity.Word">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="spelling" column="spelling" jdbcType="VARCHAR"/>
            <result property="phonetic" column="phonetic" jdbcType="VARCHAR"/>
            <result property="definition" column="definition" jdbcType="VARCHAR"/>
            <result property="exampleSentence" column="example_sentence" jdbcType="VARCHAR"/>
            <result property="wordType" column="word_type" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,spelling,phonetic,
        definition,example_sentence,word_type,
        create_time,update_time
    </sql>
</mapper>
